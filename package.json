{
  "name": "@fynyky/elementary",
  "version": "1.0.0",
  "description": "Simple reactive front-end library",
  "type": "module",
  "source": "src/index.js",
  "main": "dist/index.js",
  "targets": {
    "main": {
      "context": "browser",
      "includeNodeModules": true,
      "optimize": true
    }
  },
  "files": [
    "dist/index.*"
  ],
  "dependencies": {
    "weak-ref-collections": "^1.2.4"
  },
  "devDependencies": {
    "mocha": "^10.2.0",
    "parcel": "^2.9.3",
    "standard": "^17.1.0"
  },
  "scripts": {
    "lint": "standard --fix",
    "build": "rm -rf dist/ && parcel build",
    "prepare": "rm -rf dist/ && parcel build",
    "pretest": "rm -rf dist/ && parcel build",
    "test": "standard && mocha test/reactor.test.js",
    "browserTest": "npm run pretest && parcel serve test/elementary.test.html",
    "prepublishOnly": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fynyky/elementary.git"
  },
  "keywords": [
    "reactive",
    "ui",
    "front-end"
  ],
  "author": "Li Hongyi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fynyky/elementary/issues"
  },
  "homepage": "https://github.com/fynyky/elementary#readme"
}
