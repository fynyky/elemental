{
  "name": "elemental",
  "version": "0.0.1",
  "description": "Simple reactive ui building without frameworks",
  "type": "module",
  "main": "dist/main.js",
  "browser": "dist/browser.js",
  "source": "src/index.js",
  "files": [ "dist/**/*" ],
  "scripts": {
    "lint": "standard --fix",
    "build": "rm -rf dist/ && parcel build",
    "pretest": "npm run lint && npm run build",
    "test": "parcel serve test/test.html",
    "prepack": "npm run lint && npm run build"
  },
  "dependencies": {
    "reactorjs": "^3.0.0"
  },
  "devDependencies": {
    "parcel": "^2.8.1",
    "standard": "^17.0.0"
  },
  "targets": {
    "browser": {
      "context": "browser",
      "includeNodeModules": true,
      "optimize": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fynyky/elemental.git"
  },
  "bugs": {
    "url": "https://github.com/fynyky/elemental/issues"
  },
  "homepage": "https://github.com/fynyky/elemental#readme",
  "author": "Li Hongyi",
  "license": "MIT"
}
